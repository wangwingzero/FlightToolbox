        "selectedIconPath": "images/tab-calculator-active.png",
        "text": "计算工具"
      },
      {
        "pagePath": "pages/cockpit/index",
        "iconPath": "images/tab-cockpit.png",
        "selectedIconPath": "images/tab-cockpit-active.png",
        "text": "驾驶舱"
      },
      {
        "pagePath": "pages/operations/index",
        "iconPath": "images/tab-enroute-flight.png",
        "selectedIconPath": "images/tab-enroute-flight-active.png",
        "text": "通信"
      },
      {
        "pagePath": "pages/home/index",
        "iconPath": "images/tab-home.png",
        "selectedIconPath": "images/tab-home-active.png",
        "text": "我的首页"
      }
    ]
  },
  "style": "v2",
  "componentFramework": "glass-easel",
  "lazyCodeLoading": "requiredComponents",
  "debug": false,
  "permission": {
    "scope.userLocation": {
      "desc": "驾驶舱功能需要获取您的位置信息以显示GPS数据"
    }
  },
  "requiredPrivateInfos": [
    "getLocation",
    "chooseLocation",
    "startLocationUpdate",
    "onLocationChange"
  ]
