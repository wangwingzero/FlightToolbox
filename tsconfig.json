{
  "compilerOptions": {
    "target": "ES5",
    "lib": ["ES5", "ES2015.Promise", "ES2015.Iterable", "ES2015.Collection"],
    "module": "CommonJS",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "allowJs": true,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noImplicitThis": false,
    "strictNullChecks": false
  },
  "include": [
    "miniprogram/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "miniprogram_npm",
    "miniprogram/**/*.js"
  ]
}
